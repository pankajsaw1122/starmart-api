        <div class="container-fluid">
            <h2>Products</h2>
            <div class="card">
                <div class="card-header">
                    <h6>Add Product</h6>
                </div>
                <div class="card-body">
                    <div class="content">
                        <p>
                            <span class="left-text"> URL: </span>
                            <span class="right-text"> /product/addProduct</span>
                        </p>
                        <p>
                            <span class="left-text"> Method: </span>
                            <span class="right-text"> POST</span>
                        </p>
                        <p>
                            <span class="left-text"> Description: </span>
                            <span class="right-text"> to add products</span>
                        </p>
                        <p>
                            <span class="left-text"> Parameter: </span>
                        </p>
                        <div class="table-container">
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th>Field</th>
                                        <th>Type</th>
                                        <th>Description</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>departmentId</td>
                                        <td>Int</td>
                                        <td>ID of department</td>
                                    </tr>
                                    <tr>
                                        <td>subDepartmentId</td>
                                        <td>Int</td>
                                        <td>ID of sub department</td>
                                    </tr>
                                    <tr>
                                        <td>subCategoryId</td>
                                        <td>Int</td>
                                        <td>ID of sub Category</td>
                                    </tr>
                                    <tr>
                                        <td>productName</td>
                                        <td>String</td>
                                        <td>Name of product</td>
                                    </tr>
                                    <tr>
                                        <td>brand</td>
                                        <td>String</td>
                                        <td>brand of product</td>
                                    </tr>
                                    <tr>
                                        <td>description</td>
                                        <td>String</td>
                                        <td>Discription of Product</td>
                                    </tr>
                                    <tr>
                                        <td>regularPrice</td>
                                        <td>Double</td>
                                        <td>Regular Price of Product</td>
                                    </tr>
                                    <tr>
                                        <td>salePrice</td>
                                        <td>Double</td>
                                        <td>Sale Price of Product</td>
                                    </tr>
                                    <tr>
                                        <td>taxStatus</td>
                                        <td>Double</td>
                                        <td>Sale Price of Product</td>
                                    </tr>
                                    <tr>
                                        <td>taxClass</td>
                                        <td>Double</td>
                                        <td>Sale Price of Product</td>
                                    </tr>
                                    <tr>
                                        <td>sku</td>
                                        <td>Double</td>
                                        <td>sku of Product</td>
                                    </tr>
                                    <tr>
                                        <td>manageStock</td>
                                        <td>Boolean</td>
                                        <td>Stock of Product</td>
                                    </tr>
                                    <tr>
                                        <td>stockStatus</td>
                                        <td>String</td>
                                        <td>Stock status of Product</td>
                                    </tr>
                                    <tr>
                                        <td>soldIndv</td>
                                        <td>Boolean</td>
                                        <td>Sold Individually Product</td>
                                    </tr>
                                    <tr>
                                        <td>weight</td>
                                        <td>String</td>
                                        <td>Weight (kg) of Product</td>
                                    </tr>
                                    <tr>
                                        <td>dimension</td>
                                        <td>String</td>
                                        <td>Dimension of Product</td>
                                    </tr>
                                    <tr>
                                        <td>shippingClass</td>
                                        <td>String</td>
                                        <td>Shipping class of Product</td>
                                    </tr>
                                    <tr>
                                        <td>upsells</td>
                                        <td>String</td>
                                        <td>---</td>
                                    </tr>
                                    <tr>
                                        <td>crossSells</td>
                                        <td>String</td>
                                        <td>----</td>
                                    </tr>
                                    <tr>
                                        <td>productImage</td>
                                        <td>String</td>
                                        <td>path of image</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <p>
                            <span class="left-text"> Test: </span>
                        </p>
                        <div class="form-container">
                            <form class="form-inline" enctype="multipart/form-data" action="/products/addProducts" method="POST">
                                <label class="pad12">departmentId:</label>
                                <input type="number" class="form-control" name="departmentId">

                                <label class="pad12">subDepartmentId:</label>
                                <input type="number" class="form-control" name="subDepartmentId">

                                <label class="pad12">subCategoryId:</label>
                                <input type="number" class="form-control" name="subCategoryId">

                                <label class="pad12">productName:</label>
                                <input type="email" class="form-control" name="productName">

                                <label class="pad12">netWeight:</label>
                                <input type="text" class="form-control" name="netWeight">

                                <label class="pad12">description:</label>
                                <input type="text" class="form-control" name="description">

                                <label class="pad12">productImage:</label>
                                <input type="file" id="productImage" name="productImage" />

                                <button type="submit" class="btn btn-primary left-marg">Submit</button>
                            </form>
                        </div>

                    </div>
                </div>
            </div>

